                <IntegerInput
                    label=<%- safeString(title) %>
                    min={<%- min %>}
                    max={<%- max %>}
                    value={this.state.input<%- upperCaseFirst(inputID) %>}
                    invalidLines={
                        this.state.invalidInputs.get(<%- safeString(inputID) %>) || new Set()
                    }
                    onChange={set<%- upperCaseFirst(inputID) %>}
                />
