/*
 * Asset Handling
 */

declare module '*.svg' {
    const src: string;
    export default src;
}

declare module '*.png' {
    const src: string;
    export default src;
}

declare module '*.jpg' {
    const src: string;
    export default src;
}

declare module '*.jpeg' {
    const src: string;
    export default src;
}

declare module '*.gif' {
    const src: string;
    export default src;
}

declare module '*.lp' {
    const src: string;
    export default src;
}

/*
 * General
 */

type BaseInputKeys = <%-
        Object.entries(inputBase).map(([k, v]) => 
            `"${k}"`
        ).join(" | ")
-%>;
type ConstrainedInputKeys = <%-
        Object.entries(inputConstrained).map(([k, v]) =>
            `"${k}"`
        ).join(" | ")
-%>;
type AllInputKeys = BaseInputKeys | ConstrainedInputKeys;

